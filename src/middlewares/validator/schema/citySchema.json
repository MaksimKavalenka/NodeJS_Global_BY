{
  "title": "City Schema",
  "type": "object",
  "properties": {
    "name": {
      "type": "string"
    },
    "country": {
      "type": "string"
    },
    "capital": {
      "type": "boolean"
    },
    "lat": {
      "type": "number",
      "minimum": -90,
      "maximum": 90
    },
    "long": {
      "type": "number",
      "minimum": -180,
      "maximum": 180
    }
  },
  "required": ["name", "country", "capital", "lat", "long"]
}
