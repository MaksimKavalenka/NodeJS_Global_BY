    "express-session": "^1.15.6",
    "glob": "^7.1.2",
    "http-status": "^1.0.1",
    "i18n": "^0.8.3",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.4",
    "minimist": "^1.2.0",
    "mongodb": "^2.2.33",
    "mongoose": "^4.13.4",
    "nodemon": "^1.12.1",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth": "^1.0.0",
    "passport-local": "^1.0.0",
